{% extends 'base.html.twig' %}

{% block title %}Demandeur{% endblock %}

{% block body %}
    <h1>Demandeur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ demandeur.id }}</td>
            </tr>
            <tr>
                <th>Noms</th>
                <td>{{ demandeur.Noms }}</td>
            </tr>
            <tr>
                <th>DateNaiss</th>
                <td>{{ demandeur.DateNaiss ? demandeur.DateNaiss|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ demandeur.Tel }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ demandeur.Adresse }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('demandeur_index') }}">back to list</a>

    <a href="{{ path('demandeur_edit', {'id': demandeur.id}) }}">edit</a>

    {{ include('demandeur/_delete_form.html.twig') }}
{% endblock %}
